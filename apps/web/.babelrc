{
	"presets": [
		"es2015",
		"react"
	],
	"plugins": [
		"babel-plugin-transform-object-rest-spread"
	],
	"env": {
		"development": {
			"plugins": [
				[
					"react-transform",
					{
						"transforms": [
							{
								"transform": "react-transform-hmr",
								"imports": [ "react" ],
								"locals": [ "module" ]
							}
						]
					}
				]
			]
		}
	}
}
